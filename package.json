{
   "name": "react-mui-layout",
   "description": "A React responsive Layout for Material UI, based on React Mega Router.",
   "author": "Jipema",
   "private": false,
   "license": "MIT",
   "repository": {
      "type": "git",
      "url": "https://github.com/jipema/react-mui-layout.git",
      "directory": "src/react-mui-layout"
   },
   "keywords": [
      "react",
      "layout",
      "responsive",
      "material-ui",
      "router",
      "mobile",
      "webapp"
   ],
   "version": "0.1.3",
   "scripts": {
      "start": "react-scripts start",
      "build": "yarn run prepublishOnly",
      "prepublishOnly": "rm -rf dist && mkdir dist && babel ./src/react-mui-layout --out-dir ./dist --copy-files",
      "test": "echo \"No test specified yet, sorry about that.\" && exit 0"
   },
   "main": "dist/index.js",
   "files": [
      "dist/*"
   ],
   "babel": {
      "presets": [
         [
            "@babel/preset-react",
            {
               "useBuiltIns": true
            }
         ]
      ],
      "plugins": [
         "transform-remove-console"
      ]
   },
   "eslintConfig": {
      "extends": "react-app"
   },
   "np": {
      "contents": "dist"
   },
   "dependencies": {
      "@material-ui/core": "^4.8.1",
      "@material-ui/icons": "^4.5.1",
      "notistack": "^0.9.7",
      "react-helmet": "^5.2.1",
      "react-mega-router": "^1.0.5"
   },
   "peerDependencies": {
      "node-sass": "^4.13.0",
      "prop-types": "^15.7.2",
      "react": "^16.12.0",
      "react-dom": "^16.12.0",
      "react-scripts": "3.3.0"
   },
   "devDependencies": {
      "@babel/cli": "^7.6.4",
      "@babel/core": "^7.6.4",
      "@babel/preset-env": "^7.6.3",
      "@babel/preset-react": "^7.6.3",
      "@testing-library/jest-dom": "^4.2.4",
      "@testing-library/react": "^9.3.2",
      "@testing-library/user-event": "^7.1.2",
      "babel-plugin-transform-remove-console": "^6.9.4",
      "np": "*"
   },
   "browserslist": {
      "production": [
         ">0.2%",
         "not dead",
         "not op_mini all"
      ],
      "development": [
         "last 1 chrome version",
         "last 1 firefox version",
         "last 1 safari version"
      ]
   }
}
